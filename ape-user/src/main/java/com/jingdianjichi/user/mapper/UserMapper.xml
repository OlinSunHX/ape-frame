<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jingdianjichi.user.mapper.UserMapper">
    <!--这个是叫映射文件 在这里可以使用简单的xml执行命令-->
    <!--select 就是查询语句 映射到getuserpage方法 查询打印-->
    <select id="com.jingdianjichi.user.mapper.UserMapper.getUserPage" resultType="com.jingdianjichi.user.entity.po.UserPo">
        select id,name,age from user where delete_flag = 0
    </select>

</mapper>